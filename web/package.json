{
  "name": "packagedev",
  "version": "1.0.0",
  "description": "Starter project to dev and test packages for react-native and react-native-web.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run remove:react-native && next",
    "start": "next start -p $PORT",
