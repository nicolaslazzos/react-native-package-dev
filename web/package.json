  "bugs": {
    "url": "https://github.com/nicolaslazzos/react-native-package-dev/issues"
  },
  "homepage": "https://github.com/nicolaslazzos/react-native-package-dev#readme",
  "dependencies": {
    "next": "11.1.2",
    "next-images": "^1.7.0",
    "next-transpile-modules": "^6.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-native-web": "^0.15.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.2",
    "@types/react-native": "^0.63.50",
    "typescript": "^4.2.2"
  }
